{
  "name": "@redis/agent-memory-client",
  "version": "0.2.0",
  "description": "JavaScript/TypeScript client for the Agent Memory Server REST API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "redis",
    "memory",
    "agent",
    "ai",
    "llm",
    "semantic-search",
    "vector-search",
    "memory-server"
  ],
  "author": "Redis Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/redis/agent-memory-server.git",
    "directory": "agent-memory-client/agent-memory-client-js"
  },
  "bugs": {
    "url": "https://github.com/redis/agent-memory-server/issues"
  },
  "homepage": "https://redis.github.io/agent-memory-server/",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "ulid": "^2.3.0"
  }
}
